{
  "name": "weird",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start":"node src/server/index.js",
    "dev":"concurrently yarn:client:dev yarn:server:dev",
    "client:dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "client:build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "server:dev": "nodemon --config config/nodemon.config.json",
    "cypress:open": "cypress open --config-file config/cypress.config.js",
    "cypress:run":"cypress run --config-file config/cypress.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "cypress": "^10.1.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.30.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.16",
    "style-loader": "^3.3.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "express": "^4.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
